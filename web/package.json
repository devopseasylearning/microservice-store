    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.0.6",
    "core-js": "^3.18.2",
    "css-loader": "^5.2.7",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "js-cookie": "^3.0.1",
    "mini-css-extract-plugin": "^2.2.0",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "preact": "^10.0.0",
    "preact-router": "^3.0.1",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.2.0",
    "tailwindcss": "^2.2.19",
    "webpack": "^5.62.1",
    "webpack-cli": "^4.7.2",
    "webpack-manifest-plugin": "^4.0.2"
  }
