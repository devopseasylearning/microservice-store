{
  "name": "microservice-store",
  "version": "0.0.0",
  "description": "Microservice Store",
  "scripts": {},
  "author": {
    "name": "Glenn de Haan",
    "email": "glenn@dehaan.cloud",
    "url": "https://glenndehaan.com/"
  },
  "license": "MIT",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "no-console": 0,
      "react/prop-types": 0,
      "comma-dangle": [
        "error",
        "never"
      ],
      "indent": [
        "error",
        4
      ]
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "settings": {
      "react": {
        "pragma": "h",
        "version": "16.0"
      }
    }
  },
  "eslintIgnore": [
    "public/dist"
  ],
  "dependencies": {
    "express": "^4.17.1",
    "mini-css-extract-plugin": "^2.2.0",
    "node-fetch": "^3.0.0",
    "node-json-db": "^1.4.1",
    "rimraf": "^3.0.2",
    "simple-node-logger": "^21.8.12"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^8.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.24.0",
    "nodemon": "^2.0.12"
  }
}
